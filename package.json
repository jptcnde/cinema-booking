{
  "name": "cinema-ticket-booking",
  "version": "0.1.0",
  "private": false,
  "keywords": [
    "react",
    "nodejs",
    "lambda",
    "aws",
    "s3",
    "javascript",
    "dynamodb",
    "material-ui",
    "formik",
    "testing-library",
    "jest"
  ],
  "scripts": {
    "start-dev": "BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start-db": "json-server -p 4800 -w mock-data.json -i id",
    "start-mock": "concurrently \"npm run start-dev\" \"npm run start-db\"",
    "start": "concurrently \"npm run start-dev\" \"serverless offline --port 4800\"",
    "lint:eslint": "eslint src --fix --ignore-pattern .gitignore"
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint:eslint --",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "1-liners": "^0.4.2",
    "@auth0/auth0-spa-js": "^1.6.0",
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.8.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/pickers": "^3.2.8",
    "@material-ui/styles": "^4.7.1",
    "@material-ui/system": "^4.7.1",
    "@rematch/core": "^1.3.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "aws-sdk": "^2.593.0",
    "clsx": "^1.0.4",
    "date-fns": "^2.8.1",
    "faker": "^4.1.0",
    "formik": "^2.0.8",
    "lodash": "^4.17.15",
    "nodemailer": "^6.4.2",
    "numbro": "^2.1.2",
    "prop-types": "^15.7.2",
    "query-string": "^6.9.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-jss": "^10.0.0",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "redux-logger": "^3.0.6",
    "serverless-apigw-binary": "^0.4.4",
    "serverless-dotenv-plugin": "^2.1.1",
    "serverless-offline": "^5.12.1",
    "serverless-plugin-tables": "^1.0.2",
    "ts-pnp": "^1.1.5",
    "ua-parser-js": "^0.7.20",
    "uuid": "^3.3.3",
    "yup": "^0.28.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.0.1",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "husky": "^3.1.0",
    "json-server": "^0.15.1",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "serverless": "^1.60.0"
  }
}
